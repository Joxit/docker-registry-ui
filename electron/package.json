{
  "name": "docker-registry-ui",
  "version": "1.4.8",
  "productName": "Registry UI",
  "description": "Electron Application for Docker Registry UI",
  "main": "index.js",
  "scripts": {
    "start": "electron ./",
    "rebuild": "electron-rebuild -f -w keytar",
    "package": "electron-packager --overwrite .",
    "sync": "copyfiles ../dist/* ../dist/**/* out",
    "dist": "npm install && npm run rebuild && npm run sync && npm run package"
  },
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "keytar": "^5.6.0"
  },
  "devDependencies": {
    "copyfiles": "^2.2.0",
    "electron": "^8.0.0",
    "electron-builder": "^22.6.0",
    "electron-packager": "^14.2.1",
    "electron-rebuild": "^1.10.1"
  },
  "keywords": [
    "electron"
  ],
  "author": "",
  "license": "AGPL-3.0"
}
