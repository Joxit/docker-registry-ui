{
  "name": "docker-registry-ui",
  "version": "2.1.0",
  "scripts": {
    "serve": "cross-env NODE_ENV=development ROLLUP_SERVE=true rollup -c -w",
    "start:dev": "cross-env NODE_ENV=development rollup -c -w",
    "start:prod": "cross-env NODE_ENV=production rollup -c -w",
    "start": "npm run start:prod",
    "start:sources": "cross-env SOURCEMAP=true npm run start:prod",
    "build:dev": "cross-env NODE_ENV=development rollup -c",
    "build:prod": "cross-env NODE_ENV=production rollup -c",
    "build": "npm run build:prod",
    "build:sources": "cross-env SOURCEMAP=true npm run build:prod",
    "build:electron": "npm run build && cd examples/electron && npm install && npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Joxit/docker-registry-ui.git"
  },
  "author": "Jones Magloire (Joxit)",
  "license": "AGPL-3.0",
  "description": "A web UI for private docker registry",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@riotjs/compiler": "^6.1.3",
    "@riotjs/observable": "^4.1.1",
    "@riotjs/route": "^8.0.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-html": "^0.2.4",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "core-js": "^3.19.1",
	"cross-env": "^7.0.3",
    "js-beautify": "^1.14.0",
    "node-sass": "^7.0.1",
    "riot": "^6.1.2",
    "riot-mui": "github:joxit/riot-5-mui#4d68d7f",
    "rollup": "^2.59.0",
    "rollup-plugin-app-utils": "^1.0.6",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-riot": "^6.0.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
